/* pages/index/search/index.wxss */
page{
  background: #fafafa;
}
.topBox{
  background: #fff;
  padding: 24rpx 40rpx;
  justify-content: space-between;
}
.searchBox{
  height: 60rpx;
  border-radius: 60rpx;
  flex:1;
  border: none;
  padding-left: 20rpx;
  background: #ececec;
  margin-right: 20rpx;
}
.searchBox input{
  width: 100%;
  margin-left: 20rpx;
}
.main .keybox{
  margin:40rpx 0; 
}
.keybox .keys{
  background: #56be80;
  color: #fff;
  padding: 0 20rpx;
  border-radius: 60rpx;
  margin-right: 40rpx;
  white-space: nowrap;
  margin-bottom: 20rpx;
}
.results{
  background: #fff;
}